<%= link_to image_tag(avatar_url(@friend)), @friend %>
<%= link_to @friend.username, @friend %>
<p><%= @friend.created_at.strftime("Joined on: %m/%d/%Y") %></p>
<p><%= @friend.last_sign_in_at.strftime("Last signed in: %m/%d/%Y at %I:%M%p") %></p>
<p>Pets: <%= @friend.pets.length %></p>
<p>Score: <%= @friend.points%></p>


